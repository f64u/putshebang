#!/home/fadel/Development/github.com/faddyy/PythonPrograms/putshebang/putshebang/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'putshebang','console_scripts','putshebang'
__requires__ = 'putshebang'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('putshebang', 'console_scripts', 'putshebang')()
    )
